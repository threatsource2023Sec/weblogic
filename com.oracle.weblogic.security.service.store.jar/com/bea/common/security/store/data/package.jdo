<?xml version="1.0"?>
<jdo>
    <package name="com.bea.common.security.store.data">
        <class name="Top" objectid-class="TopId">
            <inheritance strategy="subclass-table"/>
            <field name="createTimestamp"/>
            <field name="modifyTimestamp" value-strategy="version"/>
        </class>

        <class name="DomainRealmScope" objectid-class="DomainRealmScopeId">
            <inheritance strategy="subclass-table"/>
            <field name="domainName" primary-key="true">
                <extension vendor-name="com.bea.common.security" key="ldap-attr" value="dc"/>
            </field>
            <field name="realmName" primary-key="true">
                <extension vendor-name="com.bea.common.security" key="ldap-attr" value="ou"/>
            </field>
        </class>

        <class name="RegistryScope" objectid-class="RegistryScopeId">
            <inheritance strategy="subclass-table"/>
            <field name="registryName" primary-key="true">
                <extension vendor-name="com.bea.common.security" key="ldap-attr" value="ou"/>
            </field>
        </class>

        <class name="PKITypeScope" objectid-class="PKITypeScopeId">
            <inheritance strategy="subclass-table"/>
            <field name="typeName" primary-key="true">
                <extension vendor-name="com.bea.common.security" key="ldap-attr" value="ou"/>
            </field>
        </class>

        <class name="XACMLTypeScope" objectid-class="XACMLTypeScopeId">
            <inheritance strategy="subclass-table"/>
            <field name="typeName" primary-key="true">
                <extension vendor-name="com.bea.common.security" key="ldap-attr" value="ou"/>
            </field>
        </class>

        <class name="BEASAMLPartner" objectid-class="BEASAMLPartnerId">
            <inheritance strategy="subclass-table"/>
            <field name="cn" primary-key="true"/>
            <field name="beaSAMLPartnerEnabled"/>
            <field name="beaSAMLPartnerDescription"/>
        </class>

        <class name="Credential" objectid-class="CredentialId">
            <inheritance strategy="subclass-table"/>
            <field name="credentialName" primary-key="true"/>
        </class>

        <class name="CredentialMap" objectid-class="CredentialMapId">
            <inheritance strategy="subclass-table"/>
            <field name="cn" primary-key="true">
                <extension vendor-name="com.bea.common.security" key="ldap-escaping" value="com.bea.common.ldap.escaping.WLSEscapingFactory"/>
            </field>
            <field name="resourceName"/>
            <field name="applicationName"/>
            <field name="moduleName"/>
            <field name="wlsCreatorInfo"/>
            <field name="wlsCollectionName">
                <extension vendor-name="com.bea.common.security" key="ldap-escaping" value="com.bea.common.ldap.escaping.WLSEscapingFactory"/>
            </field>
        </class>

        <class name="XACMLEntry" objectid-class="XACMLEntryId">
            <inheritance strategy="subclass-table"/>
            <field name="xacmlVersion" primary-key="true">
                <extension vendor-name="com.bea.common.security" key="ldap-rdn-cont" value="true"/>
            </field>
            <field name="cn" primary-key="true">
                <extension vendor-name="com.bea.common.security" key="ldap-escaping" value="com.bea.common.ldap.escaping.XACMLEscapingFactory"/>
            </field>
            <field name="xacmlResourceScope">
                <collection element-type="java.lang.String"/>
                <extension vendor-name="com.bea.common.security" 
                    key="ldap-auxiliary" value="xacmlResourceScoping"/>
                <extension vendor-name="com.bea.common.security" key="ldap-escaping" value="com.bea.common.ldap.escaping.XACMLEscapingFactory"/>
            </field>
            <field name="xacmlDocument" default-fetch-group="true"/>
            <field name="xacmlStatus"/>
            <field name="wlsCreatorInfo"/>
            <field name="wlsCollectionName">
                <extension vendor-name="com.bea.common.security" key="ldap-escaping" value="com.bea.common.ldap.escaping.WLSEscapingFactory"/>
            </field>
            <field name="wlsXmlFragment"/>
        </class>

        <class name="BEASAMLAssertingParty" table="BEASAMLAP" objectid-class="BEASAMLAssertingPartyId">
            <extension vendor-name="com.bea.common.security" 
                key="ldap-objectclass" value="beaSAMLAssertingParty,beaSAMLPartner"/>
            <extension vendor-name="com.bea.common.security" 
                key="ldap-insert-constant-rdn" value="realmName ou=SAMLAssertingPartyRegistry"/>
            <inheritance strategy="new-table"/>
            <version column="MTS" strategy="date-time"/>
            <field name="Top.createTimestamp">
                <column name="CTS"/>
            </field>
            <field name="Top.modifyTimestamp">
                <column name="MTS"/>
            </field>
            <field name="DomainRealmScope.domainName" primary-key="true">
                <column name="DOMN" jdbc-type="VARCHAR" length="128"/>
            </field>
            <field name="DomainRealmScope.realmName" primary-key="true">
                <column name="REALMN" jdbc-type="VARCHAR"  length="128"/>
            </field>
            <field name="RegistryScope.registryName" primary-key="true">
                <column name="REGN" jdbc-type="VARCHAR"  length="128"/>
            </field>
            <field name="BEASAMLPartner.cn" primary-key="true">
                <column name="CN" jdbc-type="VARCHAR"  length="400"/>
            </field>
            <field name="BEASAMLPartner.beaSAMLPartnerEnabled">
                <column name="SPE" jdbc-type="VARCHAR" length="16"/>
            </field>
            <field name="BEASAMLPartner.beaSAMLPartnerDescription">
                <column name="SPD" jdbc-type="VARCHAR" length="512"/>
            </field>
            <field name="beaSAMLProfile">
                <column name="SP" jdbc-type="VARCHAR" length="32"/>
            </field>
            <field name="beaSAMLTargetURL">
                <column name="STU" jdbc-type="VARCHAR" length="512"/>
            </field>
            <field name="beaSAMLIssuerURI">
                <column name="SIU" jdbc-type="VARCHAR" length="512"/>
            </field>
            <field name="beaSAMLSourceId">
                <column name="SSI" jdbc-type="VARCHAR" length="512"/>
            </field>
            <field name="beaSAMLIntersiteTransferURL">
                <column name="SITU" jdbc-type="VARCHAR" length="512"/>
            </field>
            <field name="beaSAMLAssertionRetrievalURL">
                <column name="SARU" jdbc-type="VARCHAR" length="512"/>
            </field>
            <field name="beaSAMLAuthUsername">
                <column name="SAU" jdbc-type="VARCHAR" length="128"/>
            </field>
            <field name="beaSAMLAuthPassword">
                <column name="SAP" jdbc-type="VARCHAR" length="128"/>
            </field>
            <field name="beaSAMLSignedAssertions"> <!-- true/false -->
                <column name="SSA" jdbc-type="VARCHAR" length="16"/>
            </field>
            <field name="beaSAMLAssertionSigningCertAlias">
                <column name="SASCA" jdbc-type="VARCHAR" length="128"/>
            </field>
            <field name="beaSAMLProtocolSigningCertAlias">
                <column name="SPSCA" jdbc-type="VARCHAR" length="128"/>
            </field>
            <field name="beaSAMLNameMapperClass">
                <column name="SNMC" jdbc-type="VARCHAR" length="128"/>
            </field>
            <field name="beaSAMLGroupsAttributeEnabled">
                <column name="SGAE" jdbc-type="VARCHAR" length="16"/>
            </field>
            <field name="beaSAMLVirtualUserEnabled">
                <column name="SVUE" jdbc-type="VARCHAR" length="16"/>
            </field>
            <field name="beaSAMLPersistentARSConnEnabled">
                <column name="SPARSCE" jdbc-type="VARCHAR" length="16"/>
            </field>
            <field name="beaSAMLIntersiteTransferParams" table="BEASAMLAP_ITP" default-fetch-group="true">
                <collection element-type="java.lang.String"/>
                <join outer="true">
                <column name="DOMN" target="DOMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REALMN" target="REALMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REGN" target="REGN" jdbc-type="VARCHAR" length="128"/>
                <column name="CN" target="CN" jdbc-type="VARCHAR" length="400"/>
                </join>
                <element>
                    <column name="SITP" jdbc-type="VARCHAR" length="512"/>
                </element>
            </field>
            <field name="beaSAMLRedirectURIs" table="BEASAMLAP_RURI" default-fetch-group="true">
                <collection element-type="java.lang.String"/>
                <join outer="true">
                <column name="DOMN" target="DOMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REALMN" target="REALMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REGN" target="REGN" jdbc-type="VARCHAR" length="128"/>
                <column name="CN" target="CN" jdbc-type="VARCHAR" length="400"/>
                </join>
                <element>
                    <column name="SRURI" jdbc-type="VARCHAR" length="512"/>
                </element>
            </field>
            <field name="beaSAMLAudienceURI" table="BEASAMLAP_AURI" default-fetch-group="true">
                <collection element-type="java.lang.String"/>
                <join outer="true">
                <column name="DOMN" target="DOMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REALMN" target="REALMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REGN" target="REGN" jdbc-type="VARCHAR" length="128"/>
                <column name="CN" target="CN" jdbc-type="VARCHAR" length="400"/>
                </join>
                <element>
                    <column name="SAURI" jdbc-type="VARCHAR" length="512"/>
                </element>
            </field>
        </class>


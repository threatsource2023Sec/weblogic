                <column name="DOMN" target="DOMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REALMN" target="REALMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REGN" target="REGN" jdbc-type="VARCHAR" length="128"/>
                <column name="CN" target="CN" jdbc-type="VARCHAR" length="400"/>
                </join>
                <element>
                    <column name="SACP" jdbc-type="VARCHAR" length="512"/>
                </element>
            </field>
            <field name="beaSAMLAudienceURI" table="BEASAMLRP_AU" default-fetch-group="true">
                <collection element-type="java.lang.String"/>
                <join outer="true">
                <column name="DOMN" target="DOMN" jdbc-type="VARCHAR" length="128"/>
                <column name="REALMN" target="REALMN" jdbc-type="VARCHAR" length="128"/>
